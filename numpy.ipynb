   "source": [
    "def sum_prod(X, V):\n",
    "    matrix = np.array(X)\n",
    "    vector = np.array(V)\n",
    "    result = matrix.dot(vector)\n",
    "\n",
    "    return f'Результат перемножения: {result[0]},{result[1]}'\n",
    "\n",
    "\n",
    "print(sum_prod([[4, 1], [2, 2]], [1, 2])) # Результат перемножения: 6,6\n",
    "print(sum_prod([[2, 5], [1, 6]], [1, 2])) # Результат перемножения: 12,13"
   ]
  },
